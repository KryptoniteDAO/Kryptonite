{
  "scripts": {
    "deploy:all": "npm run deploy:swap && npm run deploy:staking && npm run deploy:market && npm run deploy:convert && npm run deploy:swap",
    "deploy:staking": "node --dns-result-order=ipv4first --loader ts-node/esm ./deploy_staking_contracts.ts --require ./suppress-experimental.cjs",
    "deploy:market": "node --dns-result-order=ipv4first --loader ts-node/esm ./deploy_market_contracts.ts --require ./suppress-experimental.cjs",
    "deploy:convert": "node --dns-result-order=ipv4first --loader ts-node/esm ./deploy_convert_contracts.ts --require ./suppress-experimental.cjs",
    "deploy:swap": "node --dns-result-order=ipv4first --loader ts-node/esm ./deploy_swap_extention.ts --require ./suppress-experimental.cjs",
    "verify:all": "npm run verify:swap && npm run verify:staking && npm run verify:market && npm run verify:convert",
    "verify:staking": "node --dns-result-order=ipv4first --loader ts-node/esm ./verify_staking_contracts.ts --require ./suppress-experimental.cjs",
    "verify:market": "node --dns-result-order=ipv4first --loader ts-node/esm ./verify_market_contracts.ts --require ./suppress-experimental.cjs",
    "verify:convert": "node --dns-result-order=ipv4first --loader ts-node/esm ./verify_convert_contracts.ts --require ./suppress-experimental.cjs",
    "verify:swap": "node --dns-result-order=ipv4first --loader ts-node/esm ./verify_swap_extention.ts --require ./suppress-experimental.cjs",
    "migrate": "node --dns-result-order=ipv4first --loader ts-node/esm ./migrate.ts --require ./suppress-experimental.cjs",
    "format": "prettier --write \"./**/*.{html,ts,js,json,md}\""
  },
  "dependencies": {
    "@cosmjs/cosmwasm-stargate": "^0.30.1",
