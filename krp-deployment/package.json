    "@cosmjs/tendermint-rpc": "^0.30.1",
    "@sei-js/core": "^1.3.4",
    "@types/bech32": "^1.1.4",
    "@types/bip32": "^2.0.0",
    "@types/bip39": "^3.0.0",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.0.3",
    "ethers": "^6.4.0"
  },
  "devDependencies": {
    "@cosmwasm/ts-codegen": "^0.30.0",
